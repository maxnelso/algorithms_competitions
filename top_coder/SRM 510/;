class TheLuckyGameDivTwo {
  public: int find(int a, int b, int jLen, int bLen);
};

bool is_lucky(int a) {
  string s = to_string(a);
  for (int i = 0; i < a.size(); ++i) {
    if (a[i] != '7' && a[i] != '4') {
      return false;
    }
  }
  return true;
}

